    "xlsx": "^0.17.0",
    "axios": "^0.21.1",
    "socket.io": "^4.1.2",
    "multer": "^1.4.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7",
    "concurrently": "^6.2.0"
  }
}